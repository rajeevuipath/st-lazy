{
  "name": "@rajeevuipath/st-lazy",
  "version": "2.1.2",
  "description": "Web components for lazy loading - you can use them everywhere, without any dependency. Contains also @Lazy decorator for Stenciljs to call annotated method when component is scrolled to viewport. Web components are based on @Lazy",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm install && stencil build --es5 --docs",
    "start": "stencil build --watch --serve",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "^0.18.0"
  },
  "engines": {
    "node": "9.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajeevuipath/st-lazy"
  },
  "keywords": [
    "Stencil",
    "decrator",
    "Web components",
    "scroll",
    "lazy loading",
    "IntersectionObserver",
    "st-img"
  ],
  "author": "Micha≈Ç Kaliszewski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajeevuipath/st-lazy"
  },
  "homepage": "https://github.com/rajeevuipath/st-lazy"
}